{
  "manifest_version": 3,
  "name": "name",
  "version": "1.0",
  "description": "description",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png"
  },
  "permissions": ["tabs"],
  "action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    },
    "default_title": "default_title",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["content/index.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["resources/*.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
